String ballImage = "https://lh3.googleusercontent.com/proxy/zaTUuSBTP6zy5oSnbWvd6bUu_U_L6Rj5QBn0zyvWgPC4eh9066vmeOMpMYSkWWHOoN1lltUK1KnCK6kaLQGaSvHMeXB8SLw";

Sprite ball = new Sprite(ballImage, 100, 100, 30, 30);

 void setup() {
   size(500, 400);
 }//Runs once

 void draw() { // Default framerate 60
     background(0);
     ball.moveY(6);
     ball.display();
 }//Runs
